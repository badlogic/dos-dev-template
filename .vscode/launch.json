{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "gdb",
            "request": "attach",
            "name": "debug target",
            "executable": "${command:cmake.getLaunchTargetPath}.exe",
            "target": "localhost:5123",
            "remote": true,
            "cwd": "${workspaceRoot}",
            "gdbpath": "${workspaceFolder}/tools/gdb/gdb",
            "preLaunchTask": "task-start-dosbox"
        }
    ]
}